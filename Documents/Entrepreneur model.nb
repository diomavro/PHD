Notebook[{
Cell[BoxData[{"\"\<There are n entrepreneurs in n markets who can perfectly \
see their own markets and have a monopoly in them, and m firms which can \
enter\>\"", "\[IndentingNewLine]", "\"\<The probability that entrepreneur i \
will be sampled by each firm is:\>\"", "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   SubscriptBox["\[CapitalPi]", "i"], 
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["p", "1"], ",", 
     SubscriptBox["p", "2"], ",", "...", ",", 
     SubscriptBox["p", "i"], ",", "...", ",", 
     SubscriptBox["p", "n"]}], ")"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   FractionBox[
    RowBox[{"\[Delta]", " ", 
     SubscriptBox["\[CapitalPi]", "i"]}], 
    RowBox[{"\[Delta]", " ", 
     SubscriptBox["p", "i"]}]], "\[LessEqual]", "0"}], ";", " ", 
  RowBox[{
   FractionBox[
    RowBox[{"\[Delta]", " ", 
     SubscriptBox["\[CapitalPi]", "i"]}], 
    RowBox[{"\[Delta]", " ", 
     SubscriptBox["p", 
      RowBox[{"-", "i"}]]}]], "\[GreaterEqual]", "0"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    SubscriptBox["\[CapitalPi]", "i"], 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["p", "1"], ",", 
      SubscriptBox["p", "2"], ",", "...", ",", 
      SubscriptBox["p", "i"], ",", "...", ",", 
      RowBox[{
       RowBox[{
        SubscriptBox["p", "n"], "|", 
        SubscriptBox["p", "1"]}], "=", 
       RowBox[{
        RowBox[{
         SubscriptBox["p", "2"], "..."}], "=", 
        RowBox[{
         RowBox[{
          SubscriptBox["p", "i"], "..."}], "=", 
         SubscriptBox["p", "n"]}]}]}]}], ")"}]}], "=", " ", 
   FractionBox["1", "n"]}], 
  ";"}], "\[IndentingNewLine]", "\"\<If m = 1 then it samples k times and \
chooses the highest price. This threat of entry causes causes the nash \
equilibrium of all firms to be that they all price at 0. \n\nthis causes the \
n to not choose its profit maximizing level but to randomize. Randmizing \
uniformally around the profit maximizing level \>\"", "\[IndentingNewLine]", \
"\"\<minmax of firm 1:\>\"", "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    SubscriptBox["p", "1"], 
    RowBox[{"(", 
     RowBox[{"1", "-", 
      RowBox[{"\[CapitalPi]", 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["p", "1"], ",", 
         SubscriptBox["p", "2"]}], ")"}]}]}], ")"}]}], "=", 
   RowBox[{"(", 
    RowBox[{"1", "-", 
     RowBox[{"\[CapitalPi]", 
      RowBox[{"(", 
       RowBox[{"1", ",", 
        SubscriptBox["p", "2"]}], ")"}]}]}], ")"}]}], 
  ";"}], "\[IndentingNewLine]", "\"\<minmax of firm 2:\>\"", "\
\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    SubscriptBox["p", "2"], "\[CapitalPi]", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["p", "1"], ",", 
      SubscriptBox["p", "2"]}], ")"}]}], "=", 
   RowBox[{"\[CapitalPi]", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["p", "1"], ",", "1"}], ")"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[CapitalPi]", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["p", "1"], ",", 
      SubscriptBox["p", "2"]}], ")"}]}], "=", 
   FractionBox[
    RowBox[{"\[CapitalPi]", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["p", "1"], ",", "1"}], ")"}]}], 
    SubscriptBox["p", "2"]]}], 
  ";"}], "\[IndentingNewLine]", "\"\<Subbing in the second minmax function \
into the first:\>\"", "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    SubscriptBox["p", "1"], "-", 
    RowBox[{
     SubscriptBox["p", "1"], 
     FractionBox[
      RowBox[{"\[CapitalPi]", 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["p", "1"], ",", "1"}], ")"}]}], 
      SubscriptBox["p", "2"]]}]}], "=", 
   RowBox[{"1", "-", 
    RowBox[{"\[CapitalPi]", 
     RowBox[{"(", 
      RowBox[{"1", ",", 
       SubscriptBox["p", "2"]}], ")"}]}]}]}], 
  ";"}], "\[IndentingNewLine]"}], "Input",
